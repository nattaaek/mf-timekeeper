{
  "name": "mf-timekeeper",
  "version": "1.0.17",
  "description": "This vite plugin is a version control for module federation.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "mutation-test": "stryker run"
  },
  "keywords": [
    "vite-plugin",
    "vite plugin",
    "module federation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "typescript": "^5.5.2"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^8.2.6",
    "@stryker-mutator/html-reporter": "^3.1.0",
    "@stryker-mutator/typescript-checker": "^8.2.6",
    "@stryker-mutator/vitest-runner": "^8.2.6",
    "@types/node": "^20.14.9",
    "@types/node-fetch": "^2.6.11",
    "tsup": "^8.1.0",
    "vite": "^5.2.10",
    "vitest": "^1.5.0",
    "vitest-fetch-mock": "^0.2.2"
  }
}